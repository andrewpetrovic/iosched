<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.itic.mobile.baseactivity" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.IOSched" >
        <activity
            android:name=".ZaiqingActivity"
            android:label="@string/title_activity_zaiqing"
            android:theme="@style/Theme.IOSched.ShowZQ" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ContactsActivity"
            android:label="@string/title_activity_contacts"
            android:theme="@style/Theme.IOSched.Contacts" >
        </activity>
        <activity
            android:name=".MapActivity"
            android:label="@string/title_activity_map"
            android:theme="@style/Theme.IOSched.Map">
        </activity>
        <activity
            android:name=".DownloadDataActivity"
            android:label="@string/title_activity_download_data"
            android:theme="@style/Theme.IOSched.DownloadData">
        </activity>
        <activity
            android:name=".AboutActivity"
            android:label="@string/title_activity_about"
            android:theme="@style/Theme.IOSched.WithElevatedHeader" />
    </application>

</manifest>
